<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="120dp"
    android:layout_height="120dp"
    android:background="@drawable/check_dialog_shape"
    tools:context="com.zys.baiduphoneguard.activity.CheckUpdateActivity">
    <ImageView
        android:id="@+id/iv_bigger1"
        android:layout_width="1px"
        android:layout_height="1px"
        android:layout_centerInParent="true"
        android:src="@mipmap/load_one" />

    <ImageView
        android:id="@+id/iv_bigger2"
        android:layout_width="1px"
        android:layout_height="1px"
        android:layout_centerInParent="true"
        android:src="@mipmap/load_two" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp"
        android:text="@string/update"
        android:textColor="@color/colorPrimary" />
</RelativeLayout>
